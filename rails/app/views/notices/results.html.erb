<!-- notices#results -->
<div class="row">
  <div class="col-md-offset-2 col-md-8 text-center">
    <h1>Avisos</h1>
  </div>
</div>

<!-- resultado -->
<div class="row">
  <div class="col-md-offset-2 col-md-8">
    <% if @user.via == 'EMAIL' %>
    <div class="alert alert-info">Para activar la suscripción a los
      avisos de su automovil con placa
      <strong><%= @user.plate %></strong> debe visitar la dirección que
      le enviamos a su correo electrónico.</div>
    <% elsif @user.via == 'PHONE' %>
    <div class="alert alert-info">PHONE</div>
    <% elsif @user.via == 'TWITTER' %>
    <div class="alert alert-info">TWITTER</div>
    <% end %>
  </div>
</div>

<% if Rails.env.development? %>
<!-- debug -->
<h2>Debug</h2>
<h3>@params</h3>
<%= debug(@params) %>
<h3>@errors</h3>
<%= debug(@errors) %>
<h3>@user</h3>
<%= debug(@user) %>
<h3>@settings</h3>
<%= debug(@settings) %>
<% end %>
